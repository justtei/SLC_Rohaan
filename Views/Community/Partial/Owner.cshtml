@model string
@using MSLivingChoices.Localization
<div data-bind="with: @Model">
    <div>
        <select data-bind="options: owners, value: id, optionsText: 'text', optionsValue: 'value',
            optionsCaption: '@StaticContent.WtrMrk_Select'"></select>
        <label class="checkbox new-owner-label">
            <input type="checkbox" data-bind="checked: hasNewOwner"/>
            <span>@StaticContent.Lbl_New</span>
        </label>
    </div>
    <div data-bind="slideVisible: hasNewOwner">
        <div class="content">
            @{ Html.RenderPartial("Partial/NewOwner", "newOwner"); }
        </div>
    </div>
</div>